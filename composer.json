{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-debugbar": "^3.2",
        "caffeinated/modules": "^5.0",
        "fideloper/proxy": "~4.0",
        "gregwar/captcha": "^1.1",
        "jacobcyl/ali-oss-storage": "^2.1",
        "jaeger/querylist": "^4.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.7.*",
        "maatwebsite/excel": "^3.1",
        "overtrue/easy-sms": "^1.0",
        "overtrue/laravel-pinyin": "^4.0",
        "overtrue/laravel-wechat": "^4.0",
        "phpoffice/phpspreadsheet": "^1.5",
        "simplesoftwareio/simple-qrcode": "1.3.*",
        "workerman/workerman": "^3.5",
        "elasticsearch/elasticsearch": "^6.0"
    },
    "require-dev": {
        "filp/whoops": "~2.3",
        "fzaninotto/faker": "~1.8",
        "mockery/mockery": "~1.2",
        "nunomaduro/collision": "~2.1",
        "phpunit/phpunit": "~7.5",
        "symfony/thanks": "^1.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/helpers.php",
            "app/Support/frontend_helpers.php",
            "app/Support/backend_helpers.php",
            "app/Support/seller_helpers.php",
            "app/Support/store_helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
